{
  "id": null,
  "uid": "uploads-media",
  "title": "Uploads & Media",
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "30s",
  "time": {
    "from": "now-6h",
    "to": "now"
  },
  "templating": {
    "list": []
  },
  "panels": [
    {
      "id": 1,
      "type": "timeseries",
      "title": "File Uploads Rate",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 0
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate(file_uploads_total[5m]))"
        }
      ]
    },
    {
      "id": 2,
      "type": "timeseries",
      "title": "File Upload Failures Rate",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 0
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate(file_uploads_total{status=\"failure\"}[5m]))"
        }
      ]
    },
    {
      "id": 3,
      "type": "timeseries",
      "title": "Video Processing Duration (p95 seconds)",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 8
      },
      "targets": [
        {
          "refId": "A",
          "expr": "histogram_quantile(0.95, sum(rate(video_processing_duration_seconds_bucket[5m])) by (le))"
        }
      ]
    },
    {
      "id": 4,
      "type": "timeseries",
      "title": "Video Processing Failures Rate",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 8
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate(video_processing_failures_total[5m]))"
        }
      ]
    }
  ]
}
















