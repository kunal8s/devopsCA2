{
  "id": null,
  "uid": "exam-flow",
  "title": "Exam Flow",
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "30s",
  "time": {
    "from": "now-6h",
    "to": "now"
  },
  "templating": {
    "list": []
  },
  "panels": [
    {
      "id": 1,
      "type": "timeseries",
      "title": "Active Exams",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 0
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(exam_active_total)"
        }
      ]
    },
    {
      "id": 2,
      "type": "timeseries",
      "title": "Exam Submissions Rate",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 0
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate(exam_submissions_total[5m]))"
        }
      ]
    },
    {
      "id": 3,
      "type": "timeseries",
      "title": "Exam Submission Failures Rate",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 8
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate(exam_submission_failures_total[5m]))"
        }
      ]
    },
    {
      "id": 4,
      "type": "timeseries",
      "title": "Login Failures Rate",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 8
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate(login_failures_total[5m]))"
        }
      ]
    }
  ]
}










