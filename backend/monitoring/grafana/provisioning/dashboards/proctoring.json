{
  "id": null,
  "uid": "proctoring",
  "title": "Proctoring",
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "30s",
  "time": {
    "from": "now-6h",
    "to": "now"
  },
  "templating": {
    "list": []
  },
  "panels": [
    {
      "id": 1,
      "type": "timeseries",
      "title": "Proctoring Violations Rate",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 0
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate(proctoring_violations_total[5m]))"
        }
      ]
    },
    {
      "id": 2,
      "type": "timeseries",
      "title": "Proctoring Violations by Type",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 0
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate(proctoring_violations_total[5m])) by (type)"
        }
      ]
    },
    {
      "id": 3,
      "type": "timeseries",
      "title": "Active Proctoring Sessions",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 8
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(proctoring_sessions_active_total)"
        }
      ]
    }
  ]
}




